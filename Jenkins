pipeline{
    agent any
    stages{
        stage('Preparation'){
            steps{
            git 'https://github.com/arunmathew/JenkinsCalculator.git'
            bat 'mvn clean'
            }
        }
        stage('Test'){
        steps{
            bat 'mvn test'
            }
        }
        stage('Package'){
        steps{
            bat 'mvn package'
            }
        }
    }
}